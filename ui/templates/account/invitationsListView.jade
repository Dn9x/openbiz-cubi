ol.breadcrumb
	<% _.forEach(breadcrumbs,function(breadcrumb){ %>
	<% if(breadcrumb.url !== 'undefined' && breadcrumb.url){ %>
	li
		a(href != "<%= breadcrumb.url %>") <%= breadcrumb.name %>
	<% } else { %>
	li.active <%= breadcrumb.name %>
	<% } %>
	<% }) %>
#content
	.row
		.col-md-12
			section.panel.wizard-form.corner-flip
				header.panel-heading
					h2 <%= viewTitle%>
					p <%= viewDescription%>
				.panel-body.data-grid
				.panel-footer
					button.btn-next.btn.btn-theme.btn-record-add
						i.fa.fa-plus
						| &nbsp; Invite User &nbsp;